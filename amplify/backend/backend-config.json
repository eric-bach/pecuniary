{
  "auth": {
    "cognito": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": []
    }
  },
  "api": {
    "pecuniary": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AMAZON_COGNITO_USER_POOLS",
              "userPoolConfig": {
                "userPoolId": "authcognito"
              }
            }
          ],
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "description": "pecuniaryApiKey",
              "apiKeyExpirationDays": "365"
            }
          }
        }
      }
    }
  },
  "function": {
    "seed": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "pecuniary",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        }
      ]
    },
    "eventBus": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "pecuniary",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        },
        {
          "category": "sns",
          "resourceName": "eventBus",
          "attributes": [
            "AccountCreatedEventTopicArn",
            "AccountUpdatedEventTopicArn"
          ]
        }
      ]
    },
    "accountCreatedEvent": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "pecuniary",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        },
        {
          "category": "sns",
          "resourceName": "eventBus",
          "attributes": ["AccountCreatedEventTopicArn"]
        }
      ]
    },
    "accountUpdatedEvent": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "pecuniary",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        },
        {
          "category": "sns",
          "resourceName": "eventBus",
          "attributes": ["AccountUpdatedEventTopicArn"]
        }
      ]
    }
  },
  "hosting": {
    "S3AndCloudFront": {
      "service": "S3AndCloudFront",
      "providerPlugin": "awscloudformation"
    }
  },
  "sns": {
    "eventBus": {
      "providerPlugin": "awscloudformation"
    }
  }
}
