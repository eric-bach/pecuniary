{
  "name": "pecuniary",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/eric-bach/pecuniary",
  "bin": {
    "pecuniary": "bin/pecuniary.js"
  },
  "scripts": {
    "build": "tsc",
    "build-all": "npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm run build\"",
    "build-backend": "npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm run build\" \"backend\"",
    "build-frontend": "npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm run build\" \"frontend\"",
    "cdk": "cdk",
    "cdk-clear-context": "npm run cdk -- context --clear",
    "cinstall-all": "npm i && npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm ci\"",
    "cinstall-backend": "npm i && npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm ci\" \"backend\"",
    "cinstall-frontend": "npm i && npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm ci\" \"frontend\"",
    "clean-all": "npm run cdk-clear-context && ts-node ./helpers/cleanAll.ts",
    "deploy": "npm run build && npm run build-all && ts-node ./helpers/deploy.ts",
    "deploy-backend": "npm run quick-deploy -- -s backend",
    "deploy-frontend": "npm run quick-deploy -- -s frontend",
    "quick-deploy": "ts-node ./helpers/deploy.ts",
    "seed": "npm run build && ts-node ./helpers/seed.ts",
    "test": "npm run build && jest --watchAll=false --coverage",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@aws-appsync/utils": "^1.2.5",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.85.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.85.0-alpha.0",
    "@aws-sdk/client-cloudformation": "^3.347.1",
    "@aws-sdk/client-cloudwatch-events": "^3.347.1",
    "@aws-sdk/client-dynamodb": "^3.347.1",
    "@aws-sdk/util-dynamodb": "^3.347.1",
    "@aws-solutions-constructs/aws-cloudfront-s3": "^2.41.0",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.1",
    "@types/aws-lambda": "^8.10.116",
    "@types/jest": "^26.0.24",
    "@types/node": "^17.0.45",
    "aws-cdk": "^2.82.0",
    "aws-sdk": "^2.1392.0",
    "commander": "^9.5.0",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "jest": "^26.4.2",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.11",
    "source-map-support": "^0.5.16",
    "ts-jest": "^26.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@remix-run/architect": "^1.17.1",
    "aws-cdk-lib": "^2.85.0",
    "constructs": "^10.2.45",
    "esbuild": "^0.18.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
